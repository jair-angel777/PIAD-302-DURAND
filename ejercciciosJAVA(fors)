package ejercciosdurand;
import java.util.Random;
import java.util.Arrays;
public class EjercciosdURAND {
    
    public static void main(String[] args) {
        // TODO code application logic here
        String[] nombres = new String[] {"leonardo", "kevin", "brayan"};
        int[] edades = new int[] {16, 17, 20};

        System.out.println("Los nombres son: " + Arrays.toString(nombres));
        System.out.println("Las edades son: " + Arrays.toString(edades));
        //UTILISANDO FOR CLASICO
        for (int i = 0;i < nombres.length ; i++) {
            System.out.println("El nombre es: " + nombres[i]);
        } 
        // mostrar 5 veses cada elemento
        
        String[] cars = new String[] {"volvo", "BMW", "ford", "Mazda"};
        
        for (int i = 0;i < cars.length ; i++){
            for (int x = 0; x < 5; x++) {
             System.out.println(cars[i]);
            }
        } 
        //generar la clase ramdos, generar 10 numeros aleoatorios.
        int[] numeros = new int[10];
        for (int j = 0; j < numeros.length; j++){
            numeros[j] = (int) (Math.random() * 100) + 1;
            System.out.println(numeros[j]);
        }
        
            
    }
    
}
